extends ../layouts/default.pug

block main 
  // Section 1
  section.section-1
    .container
      .inner-wrap
        h1.inner-title(
          data-aos-duration="800" 
          data-aos="fade-up"
          )
          | AU Travel - Explore Asia, EU, and US
          br
          | Go anywhere you want
        p.inner-desc(
          data-aos-delay="150" 
          data-aos-duration="800" 
          data-aos="fade-up"
          )
          | PVTravel is a travel agency that provides a variety of travel
          |   services, including domestic and international tours. We are
          |   committed to providing the best travel experience for our customers.
        form.inner-form(
          data-aos-delay="300" 
          data-aos-duration="800" 
          data-aos="fade-up" 
          action=""
          )
          .inner-box.inner-address
            .inner-input-group
              img.inner-icon(
                alt="Address Icon" 
                src="/assets/images/icon-address.svg"
                )
              input.inner-input(
                placeholder="Where do you want to go?" 
                type="text"
                )
              i.fa-solid.fa-angle-down.inner-down
            .inner-suggest
              .inner-suggest-title
                i.fa-solid.fa-location-dot
                | Popular Destinations
              .inner-suggest-list
                .inner-item
                  .inner-item-image
                    img(
                      alt="Sydney" 
                      src="/assets/images/product-1.jpg"
                      )
                  .inner-item-content
                    .inner-item-title Sydney
                    .inner-item-desc 50 tours
                .inner-item
                  .inner-item-image
                    img(
                      alt="Melbourne" 
                      src="/assets/images/product-2.jpg"
                      )
                  .inner-item-content
                    .inner-item-title Melbourne
                    .inner-item-desc 40 tours
                .inner-item
                  .inner-item-image
                    img(
                      alt="Queensland" 
                      src="/assets/images/product-3.jpg"
                      )
                  .inner-item-content
                    .inner-item-title Queensland
                    .inner-item-desc 35 tours
          .inner-box.inner-user
            .inner-input-group
              img.inner-icon(
                alt="User Icon" 
                src="/assets/images/icon-user.svg"
                )
              input.inner-input(
                readonly="" 
                placeholder="Quantity" 
                type="text"
                )
              i.fa-solid.fa-angle-down.inner-down
            .inner-quantity
              .inner-item
                .inner-label Adult
                .inner-count
                  span.inner-down -
                  span.inner-number 1
                  span.inner-up +
              .inner-item
                .inner-label Child
                .inner-count
                  span.inner-down -
                  span.inner-number 0
                  span.inner-up +
              .inner-item
                .inner-label Infant
                .inner-count
                  span.inner-down -
                  span.inner-number 0
                  span.inner-up +
          .inner-box.inner-calendar
            .inner-input-group
              img.inner-icon(
                alt="Calendar Icon" 
                src="/assets/images/icon-calendar.svg"
                )
              input.inner-input(
                placeholder="Departure Date" 
                type="text"
                )
              i.fa-solid.fa-angle-down.inner-down
          button.inner-button
            i.fa-solid.fa-magnifying-glass
            | Search
