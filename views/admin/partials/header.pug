header.header 
  .inner-logo 
    a(href=`/${pathAdmin}/dashboard`)
      span PV
      span Travel

  .inner-wrap 
    .inner-notify 
      img(src="/admin/assets/images/icon-bell.svg" alt="Bell Icon")
      span 6
    
    .inner-account 
      .inner-avatar 
        img(src=account.avatar alt="Avatar")
      
      .inner-text 
        .inner-name #{account.fullName}
        .inner-role #{account.roleName}
    button.inner-button-menu 
      <i class="fa-solid fa-bars"></i>